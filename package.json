{
  "name": "starter-app",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "",
  "author": "Hend Mohammed",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "dev": "nodemon src/index.ts",
    "start": "npm run build && node dist/index.js",
    "format": "prettier  --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "npm run build && jasmine",
    "tests": "ENV=test db-migrate up --ENV test && tsc && jasmine && db-migrate reset",
    "tests:windows": "set ENV=test db-migrate up --env test && tsc && jasmine && db-migrate reset",
    "migrate": "db-migrate up"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jasmine": "^3.10.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.0.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.3"
  }
}
